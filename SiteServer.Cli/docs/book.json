{
  "title": "SiteServer Cli 命令行",
  "description": "SiteServer Cli 命令行用于执行一些基础性的功能，如一键备份、一键还原、6.0版本之前的系统升级到最新版本以及切换系统至不同类型的数据库等功能。",
  "author": "SiteServer CMS",
  "output.name": "site",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "readme.md"
  },
  "links": {
    "sidebar": {
      "SiteServer Cli 命令行": "/docs/cli/index.html"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "ga",
    "-sharing",
    "sharing-plus",
    "favicon",
    "splitter",
    "tbfed-pagefooter",
    "multipart",
    "edit-link"
  ],
  "pluginsConfig": {
    "sharing": {
      "douban": true,
      "linkedin": true,
      "qq": true,
      "qzone": true,
      "weibo": true,
      "all": [
        "douban",
        "weibo",
        "linkedin",
        "qq",
        "qzone"
      ]
    },
    "ga": {
      "token": "UA-1398374-1"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico",
      "appleTouch": "assets/favicon.ico",
      "appleTouchMore": {
        "120x120": "assets/favicon.ico",
        "180x180": "assets/favicon.ico"
      }
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy <a href='http://www.siteserver.cn'>SiteServer CMS</a> 2018",
      "modify_label": "修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": false
    },
    "edit-link": {
      "base": "https://github.com/siteserver/cms/edit/dev/SiteServer.CLI/docs",
      "label": "编辑此页面"
    }
  }
}